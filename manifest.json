{
    "name": "Slack - Request code review",
    "version": "1.0.0",
    "description": "Lazy request code review",
    "manifest_version": 3,
    "author": "Trung Truong",
    "permissions": [
      "tabs",
      "activeTab"
    ],
    "action":{
        "default_popup": "index.html",
        "default_title": "LSRCR"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
    }
}
